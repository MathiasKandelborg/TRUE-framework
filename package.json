{
  "private": true,
  "author": "Mathias Kandelborg",
  "name": "true-framework",
  "description": "TRUE Framework === The Really Unique & Exciting Framework (for the web)",
  "version": "0.0.0-development",
  "license": "MIT",
  "homepage": "https://github.com/MathiasKandelborg/TRUE-framework#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathiasKandelborg/TRUE-framework.git"
  },
  "bugs": {
    "url": "https://github.com/MathiasKandelborg/TRUE-framework/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && node util/fixSw.js",
    "start": "next start",
    "commit": "git-cz",
    "check:all": " prettier components pages typescript util && eslint --cache .",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.1",
    "@mui/styles": "^5.4.1",
    "@mui/system": "^5.4.1",
    "@mui/types": "^7.1.1",
    "@sanity/block-content-to-react": "3.0.0",
    "@sanity/client": "2.16.0",
    "@sanity/image-url": "1.0.1",
    "clsx": "1.1.1",
    "easy-peasy": "^5.0.3",
    "final-form": "4.20.2",
    "framer-motion": "4.1.17",
    "groq": "2.15.0",
    "mdi-material-ui": "^7.1.0",
    "next": "^12.0.10",
    "next-i18next": "^10.3.0",
    "next-pwa": "5.3.1",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "6.5.3",
    "sitemap": "7.0.0",
    "swr": "1.0.0",
    "@types/webpack-env": "1.16.2",
    "ua-parser-js": "0.7.28"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@commitlint/prompt": "13.1.0",
    "@next/eslint-plugin-next": "11.1.2",
    "@types/express": "4.17.13",
    "@types/node": "16.7.10",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.0.8",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
